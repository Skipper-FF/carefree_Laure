<div class="form_wrapper">
    <%= simple_form_for(@labour) do |f| %>
      <div class="part1">
        <%= f.input :labour_start, label: 'Mode de début du travail', as: :select, collection: Labour::LABOUR_START %>
        <%= f.input :artificial_labour, label: 'Motif de déclenchement artificiel', as: :select, collection: Labour::ARTIFICIAL_LABOUR %>
        <%= f.input :labour_drugs, label: 'Moyens utilisés', as: :select, collection: Labour::LABOUR_DRUGS %>

      </div>
      <div class="part2">
        <%= f.input :labour_end, label: 'Délivrance', as: :select, collection: Labour::LABOUR_END %>
        <%= f.input :labour_complication, label: 'Complications', as: :radio_buttons, collection: ['oui', 'non'] %>
        <%= f.input :labour_complication_type, label: 'Complications de la délivrance', as: :select, collection: Labour::LABOUR_COMPLICATION_TYPE %>
        <%= f.input :labour_actes, label: 'Actes de la délivrance', as: :select, collection: Labour::LABOUR_ACTES %>
      </div>
      <div class="part3">
        <%= f.input :anaesthesia_category, label: 'Type d\'anésthésie', as: :select, collection: Labour::ANAESTHESIA_CATEGORY %>
        <%= f.input :anaesthesia_general, label: 'Anésthésie générale', as: :select, collection: Labour::ANAESTHESIA_GENERAL %>
        <%= f.input :labour_actes, label: 'Complication lors de la délivrance ', as: :select, collection: Labour::LABOUR_ACTES %>
      </div>
      <div class="part4">

      </div>
      <div class="part5">

        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>
